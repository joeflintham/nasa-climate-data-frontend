body
  font-family Arial, sans-serif

main

  #nav

  #content
    
    #mapController
      width 100%
      
      #map
        height 400px

      #subgraphA, #subgraphB
        width 50%
      
                
    #mainGraph
      width 100%
      height 400px

      svg
        margin 0 auto
        display block
        height 400px
          
circle.clicked-point
  stroke-width 1px
  stroke #ff0000
  fill #ff0000
  fill-opacity 0.5
  
g.chart path 
  stroke-width 2px
  stroke #f00
  fill none

